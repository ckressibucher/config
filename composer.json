{
    "name": "ckr/config",
    "description": "Simple access to multi dimensional arrays, useful for configuration data",
    "require": {
        "php": ">=5.5.0",
        "ckr/arraymerger": "^1.0.0@RC"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.3",
        "phpunit/phpunit": "~4.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Claudio Kressibucher",
            "email": "ckressibucher@graphicworks.ch"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Ckr\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Ckr\\Config\\": "spec/"
        }
    }
}
